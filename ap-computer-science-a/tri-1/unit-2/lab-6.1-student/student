public class Student {
	//Instance variables
	private String myName; //private means it can only be accessed through this 	class
	private String myID;
	private int myTotalPoints;
	private int myNumGrades; //Number of grades
	private double myGPA;
    
	public Student() {
    	myName="";
    	myID="";
    	myTotalPoints=0;
    	myNumGrades=0;
    	myGPA=0;
	}
	public Student(String name, String ID) {
    	myName=name;
    	myID=ID;
    	myTotalPoints=0;
    	myNumGrades=0;
    	myGPA=0;
	}

	public void addGrade(int grade) { //"Tester" must have something entered for ()
    	// grade+=myNumGrades; // //Doesn't work
   	 
    	myTotalPoints+=grade;
    	myNumGrades+=1;
    	myGPA=(double)(myTotalPoints)/myNumGrades; //Don't write double b/c it's already done
	}
	public double getGPA() {
    	return myGPA;
	}
}