import chn.util.*;
public class Tester {
    public static void main(String[] args) {
   	 ConsoleIO console=new ConsoleIO();
   	 String statusStr;
   	 double income=0;
  	 
   	 System.out.println("Hello. Input a letter for your filing status: single (s) or married (m)?");
   	 statusStr=console.readToken();
  	 
   	 System.out.println("Input your taxable income.");
   	 double taxIncome=console.readDouble();
  	 
   	 char status=statusStr.charAt(0); //charAt converts String to char
   	 TaxPayer taxes=new TaxPayer(status, taxIncome);
  	 
   	 double sOrM=taxes.calculateTax();
  	 
   	 System.out.println("Filing status: "+statusStr);
   	 System.out.println("Taxable income = $"+taxIncome);
   	 System.out.println("Federal tax = $"+sOrM);
    }
}