import chn.util.*;
public class Tester {
    public static void main(String[] args) {
   	 ConsoleIO input=new ConsoleIO();

   	 System.out.print("Principal: $");
   	 double loan=input.readDouble();
  	 
   	 System.out.print("Time (years): ");
   	 int years=input.readInt();

   	 System.out.print("Low annual interest rate: ");
   	 double lowRate=input.readDouble();

   	 System.out.print("High annual interest rate: ");
   	 double highRate=input.readDouble();     

   	 LoanTable t1=new LoanTable(loan, years, lowRate, highRate);
   	 t1.print();

   	 // t1.setLowRate(2.5);
   	 // t1.setHighRate(5.0);

   	 // t1.print();
    }
}