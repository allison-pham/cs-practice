public class Pig extends Animal {
    private String myType;
    private String mySound;
    public Pig(String type, String sound) {
   	 myType = "pig"; //type
   	 mySound = "oink"; //sound
    }

    @Override
    public String getSound() {
   	 return mySound;
    }

    @Override
    public String getType() {
   	 return myType;
    }
}