public class BackToSchool {
    public static void main(String[] args) {
   	 Person sally=new Person("Ms. Sally", 30);
   	 System.out.println(sally);
  	 
   	 Student joe=new Student("Joe", 16, "1354213", 3.7);
   	 System.out.println(joe);

   	 Teacher p=new Teacher("Mr. P", 40, "English", 55000);
   	 System.out.println(p);

   	 CollegeStudent sara=new CollegeStudent("Sara", 17, "1369831", 3.9, "Humanities", 2);
   	 System.out.println(sara);
    }
}