class Car extends Vehicle {
    private int seat; //Number of seats for passengers

    //Constructor
    public Car(String vehicleName, String model, int seat) {
   	 super(vehicleName, model);

   	 this.seat=seat;
    }

    public int getSeat() {
   	 return seat;
    }

    public void setSeat(int seat)
    {
   	 this.seat=seat;
    }

    @Override
    public String toString() {
   	 return super.toString()+", seat: "+seat;
    }
}