    public void readFile() {
   	 //Print afterward for debugging
   	 while(reader.hasNextLine()) {
   		 String country = reader.nextLine().toLowerCase(); //Read next token
   		 countries[logLength] = country; //Loglength controls index of array
   		 logLength++; //Adding country to array
   	 }
    }
    //freq[index]++;
    
    public void fillFreq() { //Helper method
   	 //Fill the frequency
   	 for(int i = 'a'; i <= 'z'; i++) {
   	 }
    }

