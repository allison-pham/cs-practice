import javax.swing.JOptionPane;
public class ParsingWorksheet1
{
    public static void main(String[] args)
    {
   	 //The order of code matters
   	 int num;
   	 int numTripled;
   	 String input;
   	 input = JOptionPane.showInputDialog("Enter an integer value");
  	 
   	 num=Integer.parseInt(input); //missing part #1
   	 numTripled=Integer.parseInt(input)*3; //missing part #2

   	 System.out.println("You original number was " + num);
   	 System.out.println("Your number tripled is " + numTripled);
    }
}