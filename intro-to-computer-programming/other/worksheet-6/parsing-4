import javax.swing.JOptionPane;
public class ParsingWorksheet4
{
    public static void main(String[] args)
    {
   	 double num1;
   	 double num2;
   	 double product;
   	 String input;
   	 input = JOptionPane.showInputDialog("Enter a real value");
   	 num1=Double.parseDouble(input); //missing part #1
      		 
   	 input = JOptionPane.showInputDialog("Enter another real value");
   	 num2=Double.parseDouble(input); //missing part #2
   	 product=Double.parseDouble(input); //missing part #3
   	 product=num1*num2;
  	 
   	 System.out.println("You first number is " + num1);
   	 System.out.println("You second number is " + num2);
   	 System.out.println("The product of your numbers is " + product);
    }
}