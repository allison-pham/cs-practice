import javax.swing.*;
import java.text.*;
public class ForLoop13
{
    public static void main(String[] args)
    {
   	 //Problem 13: Fibonacci
   	 int input=Integer.parseInt(JOptionPane.showInputDialog("Input a number"));
   	 int start1=0;
   	 int start2=1;
   	 int start3=start1+start2;
   	 for(int i=1; i<=input; i++)
   	 {
   		 if(i<=(input-1))
   		 {
       		 System.out.print(start1+", ");
   		 }
   		 else
   		 {
       		 System.out.print(start1);
   		 }
   		 start1=start2;
   		 start2=start3;
   		 start3=start1+start3;
   	 }

   	 //Missing the first 2 numbers.
   	 //Hint: Double check the order of statements in the for loop
   	 //and which variable you should be printing.
    }
}